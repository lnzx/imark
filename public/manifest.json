{
    "manifest_version": 2,
	"name": "imark",
	"version": "0.0.1",

    "icons":
    {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    
    "browser_action":
    {
        "default_icon": "icon.png",
        "default_title": "imark"
    },

    "background": {
        "scripts": ["assets/background.js"],
        "persistent": true
    },

    "permissions": [
        "bookmarks"
    ]
}